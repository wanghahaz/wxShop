video {
  min-width: 100%;
  min-height: 100%;
  height: auto;
  width: auto;
  object-fit: fill;
}

.box_video {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  overflow-y: hidden;
}

.box_tit {
  position: fixed;
  left: 3%;
  top: 30rpx;
  z-index: 99;
}

.close, .next {
  border: 1px solid #fff;
  border-radius: 30rpx;
  width: 180rpx;
  line-height: 60rpx;
}
