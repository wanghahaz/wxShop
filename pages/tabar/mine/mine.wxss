.info {
  height: 313rpx;
  position: relative;
}

.info .bg {
  width: 100%;
  height: 100%;
}

.info .set {
  width: 42rpx;
  height: 42rpx;
  position: absolute;
  right: 32rpx;
  top: 32rpx;
}

open-data[type="userAvatarUrl"] {
  position: absolute;
  width: 142rpx;
  height: 142rpx;
  border-radius: 50%;
  top: 54rpx;
  left: 51rpx;
  box-shadow: 3px 3px 8px rgba(58, 66, 89, 0.2);
}

.getinfo {
  color: #000;
  position: absolute;
  top: 107rpx;
  left: 224rpx;
  font-size: 40rpx;
  border: none;
}

.members {
  position: relative;
  width: 92%;
  height: 182rpx;
  background: #3b3836;
  margin: 0 auto;
  top: -80rpx;
  border-radius: 18rpx;
}

.mt {
  margin-top: 28rpx;
}

.members text {
  display: inline-block;
}

.members .tit {
  color: #ffed21;
