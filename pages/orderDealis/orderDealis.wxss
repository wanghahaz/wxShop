.status {
  line-height: 194rpx;
  padding-left: 60rpx;
  color: #fff;
  background: #f18e1e;
  border-radius: 18rpx;
  margin: 10rpx auto 20rpx auto;
}
.orderDealis{
  margin-top: 20rpx;
}
.orderDealis, .goodspDealis {
  border-radius: 18rpx;
  padding: 30rpx 10rpx;
  background: #fff;
}

.goodspDealis {
  background: #fff;
  padding-top: 0;
}

.orderDealis>text, .orderDealis>view {
  margin-top: 20rpx;
}

.sum {
  border-top: 1px solid #e6e6e6;
  padding: 0 10rpx;
}

.sum>view {
  margin-top: 14rpx;
}

.dealis_order {
  padding: 30rpx 10rpx 0 10rpx;
  margin-top: 30rpx;
}

.com {
  margin: 20rpx 0;
  padding: 0 10rpx;
}

.dealis_order .goodsImg {
  height: 175rpx;
  width: 175rpx;
}

.dealis_order .goods_Dealis {
  margin-bottom: 20rpx;
}

.dealis_order .evaluation {
  border-radius: 24rpx;
  padding: 8rpx 26rpx;
  border: 1px solid #f18e1e;
}

.dealis_order .count {
  margin-top: 30rpx;
  align-items: flex-end;
}

.dealis_order .allCheck, .submit .allCheck {
  width: 38rpx;
  height: 38rpx;
  margin-right: 10rpx;
}

.dealis_order .goodsImg {
  margin-right: 40rpx;
}

.dealis_order .goodsName {
  flex: 1;
}

.dealis_order .btn {
  justify-content: flex-end;
}

.dealis_order .btn>text {
  margin-left: 20rpx;
}

.submit {
  z-index: 99;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  border-top: 1px solid #e6e6e6;
  height: 94rpx;
  background: #fff;
  padding-right: 4%;
}

.submit >text {
  width: 168rpx;
  line-height: 64rpx;
  border-radius: 32rpx;
  background: #f18e1e;
  color: #fff;
}
