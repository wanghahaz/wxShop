        <view data-path="" catchtap="toRouter" style="margin-bottom:20rpx;">
          <text>我是店铺></text>
        </view>
        <view class="goods_Dealis flex flex_nosize" wx:for="{{3}}" wx:for-item="value" wx:for-index="ind" wx:key='{{ind}}'>
