/* 购物车呼起 */

.maskContent {
  z-index: 999;
  width: 100%;
  height: 70%;
  position: fixed;
  background: #fff;
  bottom: 0;
  left: 0;
  overflow-x: hidden;
  overflow-y: auto;
  padding-bottom: 100rpx;
  transform: translateY(110%);
  -webkit-transform: translateY(110%);
  transition: all 0.3s ease;
  -webkit-transition: all 0.3s ease;
}

.maskContentAc {
  transform: translateY(0);
  -webkit-transform: translateY(0);
}

.maskContent .close {
  position: absolute;
  color: #b2b2b2;
  top: 30rpx;
  right: 30rpx;
}

.maskContent .goodsDe {
  position: relative;
  padding: 32rpx 31rpx;
  border-bottom: 1px solid #e4e4e4;
}

.maskContent .goodsImg {
  width: 246rpx;
  height: 197rpx;
}

.maskContent .goodsPr {
  color: #f18e1e;
}

.maskContent .addCard {
  width: 82%;
  line-height: 72rpx;
  background: #f18e1e;
  position: fixed;
  left: 9%;
  color: white;
  bottom: 26rpx;
  border-radius: 36rpx;
}

.maskContent >.btn_bottom {
  position: fixed;
  bottom: 20rpx;
  width: 60%;
  line-height: 70rpx;
  left: 50%;
  transform: translateX(-50%);
  color: #fff;
  overflow: hidden;
  border-radius: 32rpx;
  right: 10%;
}

.maskContent .btn_bottom text {
  flex: 1;
}

.maskContent .btn_bottom text:nth-of-type(1) {
  background: -webkit-linear-gradient(left, #fdd118, #f18e1e);
  background: -o-linear-gradient(right, #fdd118, #f18e1e);
  background: -moz-linear-gradient(right, #fdd118, #f18e1e);
  background: linear-gradient(to right, #fdd118, #f18e1e);
}

.maskContent .btn_bottom text:nth-of-type(2) {
  background: #f18e1e; /* 标准的语法 */
}
