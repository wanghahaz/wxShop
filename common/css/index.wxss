/* 推荐秒杀专区列表title样式 */

.recommended {
  position: relative;
  margin: 30rpx 0 20rpx 0;
}

.recommended::after {
  position: absolute;
  content: "";
  width: 38%;
  height: 2rpx;
  background: #e4e4e4;
  left: 0;
  top: 50%;
  transform: translateY(-50%);
}

.recommended::before {
  position: absolute;
  content: "";
  width: 38%;
  height: 2rpx;
  background: #e4e4e4;
  right: 0;
  top: 50%;
  transform: translateY(-50%);
}

/* 没有更多了 */

.noMore {
  text-align: center;
  line-height: 70rpx;
  font-size: 24rpx;
}

/* 搜索框 */

.search {
  background: #fff;
  height: 48rpx;
  border-radius: 22rpx;
  position: relative;
  display: flex;
  align-items: center;
}

.search .icon {
  margin: 0 10rpx 0 16rpx;
}

.search .border {
  border-right: 1px solid #b2b2b2;
  height: 50%;
}

.search input {
  width: 100%;
  padding-left: 10rpx;
  line-height: 45rpx;
}

.seaText {
  padding: 0 0 0 20rpx;
  color: #b2b2b2;
  display: inline-block;
  line-height: 48rpx;
}

/* 加载圈 */

.downLoding {
  margin: 50rpx auto 0 auto;
  width: 2px;
  height: 2px;
  border-radius: 1px;
  box-shadow: 0 -12px 0 3px black,
                /*上*/ 0 12px 0 1px #333,
                /*下*/ -12px 0 0 1px #333,
                /*左*/ 12px 0 0 1px #333,
                /*右*/ -9px -9px 0 1px #333,
                /*左上*/ 9px -9px 0 2px #333,
                /*右上*/ -9px 9px 0 1px #333,
                /*左下*/ 9px 9px 0 1px #333/*右下*/;
  animation: loading 2s linear 0s infinite;
  -webkit-animation: loading 2s linear 0s infinite;
  -o-animation: loading 2s linear 0s infinite;
  -moz-animation: loading 2s linear 0s infinite;
}

@keyframes loading {
  from {
    transform: rotate(0deg);
  }

  to {
    transform: rotate(360deg);
  }
}

